{
  "expo": {
    "scheme": "vibenet",
    "name": "Соціальлна мережа",
    "slug": "vibenet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "backgroundColor": "#0d133f"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Додатку потрібен доступ до фото бібліотеки для вибору зображень.",
        "NSPhotoLibraryAddUsageDescription": "Додаток потребує доступу для збереження зображень у галереї.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.liudmylamila.vibenet"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0d133f"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.liudmylamila.vibenet"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4247aa18-2c58-4444-961c-492aa13c9d9d"
      }
    }
  }
}
